    <sql id="BaseKpBaseInfo_Column_List">
		KOIB_FPDM AS koibFpdm,
		KOIB_FPHM AS koibFphm,
		KOIB_FPLXDM AS KoibFplxdm,
		KOIB_FPZT AS koibFpzt,
